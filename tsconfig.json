{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@env/*": ["src/conf/*"]
    },
    "incremental": true, 
    "typeRoots": ["node_modules/@types"]
  },
  "exclude": ["node_modules", "dist","storage"],
  "include": [ "src"]
}
