{
  "name": "sequelize-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "cpdir": "cp -r ./templates ./dist/templates && cp -r ./storage ./dist/storage  && cp ./.htaccess ./dist && cp ./.env ./dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "nest build && npm run cpdir",
    "newbuild": "nest build",
    "start": "npm run cpdir && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@mapbox/mapbox-sdk": "^0.13.0",
    "@nestjs-modules/mailer": "^1.6.0",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.6",
    "@nestjs/platform-socket.io": "^8.1.1",
    "@nestjs/websockets": "^8.1.1",
    "ask-sdk-core": "^2.12.1",
    "ask-sdk-express-adapter": "^2.12.1",
    "ask-sdk-model": "^1.38.2",
    "basic-ftp": "^4.6.6",
    "bcrypt": "^4.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "conekta": "^4.0.1",
    "dotenv": "^10.0.0",
    "ejs": "^3.0.2",
    "express-rate-limit": "^5.1.1",
    "facturapi": "^2.5.1",
    "firebase-admin": "^10.0.2",
    "geoip-lite": "^1.4.2",
    "json2csv": "^5.0.6",
    "mapbox-gl": "^2.4.0",
    "mysql2": "^2.1.0",
    "nest-winston": "^1.6.2",
    "nestjs-real-ip": "^1.0.3",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^7.3.0",
    "sequelize": "^5.21.5",
    "sequelize-typescript": "^1.1.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.13.2",
    "@nestjs/schematics": "^6.8.1",
    "@nestjs/testing": "^6.11.11",
    "@types/express": "^4.17.2",
    "@types/jest": "25.1.2",
    "@types/node": "^13.1.6",
    "@types/sequelize": "^4.28.10",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.19.1",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
